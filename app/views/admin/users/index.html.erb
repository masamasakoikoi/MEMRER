<h1>会員一覧（管理者）</h1>
    <table class='table'>
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>フォロー数</th>
          <th>フォロワー数</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
          <th>退会ボタン</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.last_name %><%= user.first_name %></td>
            <td><%= user.followings.count %></td>
            <td><%= user.followers.count %></td>
            <td><%= user.email %></td>
            <td><%= user.is_deleted %></td>
            <td><%= link_to "退会",user, method: :delete,data: {confirm: "You sure?"} %></td>
        <% end %>
          </tr>
      </tbody>
    </table>
